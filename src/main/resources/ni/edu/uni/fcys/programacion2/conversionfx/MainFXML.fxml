<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ni.edu.uni.fcys.programacion2.conversionfx.controllers.MainFXMLController">
    <stylesheets>
        <URL value="@/ni/edu/uni/fcys/programacion2/conversionfx/styles/mainfxml.css" />
    </stylesheets>
   <children>
      <BorderPane layoutX="200.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0" style="-fx-border-color: #5df2d6; -fx-background-color: #00bfa5;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnTemperature" mnemonicParsing="false" onAction="#btnTemperatureAction" prefHeight="100.0" prefWidth="90.0" style="-fx-background-color: #00bfa5; -fx-border-color: #5df2d6;" text="Temperature" textFill="#280202">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="btnCurrency" mnemonicParsing="false" onAction="#btnCurrencyAction" prefHeight="100.0" prefWidth="90.0" style="-fx-background-color: #00bfa5; -fx-border-color: #5df2d6;" text="Currency" />
               </children>
            </VBox>
         </left>
         <center>
            <VBox fx:id="vboxCenter" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
